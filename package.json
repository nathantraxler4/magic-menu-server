    "license": "ISC",
    "description": "",
    "dependencies": {
        "@apollo/server": "^4.11.0",
        "@typegoose/typegoose": "^12.9.0",
        "dotenv": "^16.4.5",
        "graphql": "^16.9.0",
        "mongoose": "^8.8.1",
        "openai": "^4.70.2"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-typescript": "^7.26.0",
        "@eslint/js": "^9.15.0",
        "@graphql-codegen/cli": "5.0.3",
        "@graphql-codegen/typescript": "4.1.1",
        "@graphql-codegen/typescript-resolvers": "4.4.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.7.8",
        "babel-jest": "^29.7.0",
        "eslint": "^9.15.0",
        "eslint-plugin-jsdoc": "^50.5.0",
        "globals": "^15.12.0",
        "jest": "^29.7.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.15.0"
    }
